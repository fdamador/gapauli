{
   Assert[ (#[[1]])(#[[2]]) == #[[3]] ],
   Assert[ (#[[2]])(#[[1]]) == #[[3]] ],
   Assert[ (#[[1]]) ** (#[[2]]) == #[[3]] ],
   Assert[ (#[[2]]) ** (#[[1]]) == #[[3]] ]
} &/@
{
   {s0, s0, Scalar[1]},
   {s0, e1, Vector[1, 1]},
   {s0, e2, Vector[1, 2]},
   {s0, e3, Vector[1, 3]},
   {s0, b23, Bivector[1, 2, 3]},
   {s0, b31, Bivector[1, 3, 1]},
   {s0, b12, Bivector[1, 1, 2]},
   {s0, t123, Trivector[1]},
   {s0, m01, s0 + e1},
   {s0, m02, s0 + b23},
   {s0, m03, s0 + t123},
   {s0, m12, e1 + b23},
   {s0, m13, e1 + t123},
   {s0, m23, b23 + t123},
   {s0, m012, s0 + e1 + b23},
   {s0, m013, s0 + e1 + t123},
   {s0, m023, s0 + b23 + t123},
   {s0, m123, e1 + b23 + t123},
   {t123, s0, Trivector[1]},
   {t123, e1, Bivector[1, 2, 3]},
   {t123, e2, Bivector[1, 3, 1]},
   {t123, e3, Bivector[1, 1, 2]},
   {t123, b23, Vector[-1, 1]},
   {t123, b31, Vector[-1, 2]},
   {t123, b12, Vector[-1, 3]},
   {t123, t123, Scalar[-1]},
   {t123, m01, t123 s0 + t123 e1},
   {t123, m02, t123 s0 + t123 b23},
   {t123, m03, t123 s0 + t123 t123},
   {t123, m12, t123 e1 + t123 b23},
   {t123, m13, t123 e1 + t123 t123},
   {t123, m23, t123 b23 + t123 t123},
   {t123, m012, t123 s0 + t123 e1 + t123 b23},
   {t123, m013, t123 s0 + t123 e1 + t123 t123},
   {t123, m023, t123 s0 + t123 b23 + t123 t123},
   {t123, m123, t123 e1 + t123 b23 + t123 t123}
} ;

{
   Assert[ (#[[1]])(#[[2]]) == #[[3]] ],
   Assert[ (#[[2]])(#[[1]]) == #[[3]] ]
} &/@
{
{2, s0 , Scalar[2]},
{2, e1 , Vector[2, 1]},
{2, e2 , Vector[2, 2]},
{2, e3 , Vector[2, 3]},
{2, b23 , Bivector[2, 2, 3]},
{2, b31 , Bivector[2, 3, 1]},
{2, b12 , Bivector[2, 1, 2]},
{2, t123 , Trivector[2]},
{2, m01 , 2 s0 + 2 e1},
{2, m02 , 2 s0 + 2 b23},
{2, m03 , 2 s0 + 2 t123},
{2, m12 , 2 e1 + 2 b23},
{2, m13 , 2 e1 + 2 t123},
{2, m23 , 2 b23 + 2 t123},
{2, m012 , 2 s0 + 2 e1 + 2 b23},
{2, m013 , 2 s0 + 2 e1 + 2 t123},
{2, m023 , 2 s0 + 2 b23 + 2 t123},
{2, m123 , 2 e1 + 2 b23 + 2 t123}
} ;
